
config usbmodeminfo modeminfo
    option defaultvid '0000'
    option defaultpid '0000'
    option targetvid '0000'
    option targetpid '0000'
    option modem_type 'Modem'
    option modeswitch '1'
    option proto 'dhcp'
    option cport 'ttyUSB0'
    option dport 'ttyUSB0'
    option ifname 'usb0'
    option message1 '11111'
    option message2 '22222'
    option message3 '33333'

config usbmodemisp modemisp
    option setisp 'auto'
    option locindex '0'
    option ispindex '0'
    option pinlock '0'
    option pincode '1234'
    option dial_num '*99#'
    option apn 'yourapn.com'
    option username 'myname'
    option password 'password'
    option authentype '0'
    option modem_signal '15'

config usbmodemisp modemconf
    option conn_type '0'
    option modemstatus '0'
    option connectmode 'auto'
    option maxidletime '15'
    option mtu '1480'
    option manualdns 'false'
    option primarydns '8.8.8.8'
    option seconddns '0.0.0.0'
	option connectstatus '0'

